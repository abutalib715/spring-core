<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean class="com.spring.core.auto.wire.Address" name="address">
		<property name="state" value="Temp state" />
		<property name="city" value="Temp City" />
	</bean>
	
	<bean class="com.spring.core.auto.wire.Employee" name="employee" autowire="byName"></bean>


</beans>