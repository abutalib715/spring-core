<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c	="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.spring.core.ci.Person" name="person">
		<constructor-arg value="Talib" type="java.lang.String"/>
		<constructor-arg value="31231" type="java.lang.Integer" />
		<constructor-arg ref="certificate" />
		<constructor-arg>
			<list>
				<value>SSC</value>
				<value>HSC</value>
			</list>
		</constructor-arg>
	</bean>

	<bean class="com.spring.core.ci.Certificate" name="certificate" c:name="HSC">
	</bean>

	<bean class="com.spring.core.ci.Addition" name="addition">
		<constructor-arg value="54" type="int" index="1"></constructor-arg>
		<constructor-arg value="19" type="int" index="0"></constructor-arg>
	</bean>

</beans>